        "backgroundColor": "#0f172a"
      },
      "package": "com.vibecoding.weatherapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8ffa1ec7-de2d-4ed9-8ac3-e25b78885a41"
      }
    }
  }
